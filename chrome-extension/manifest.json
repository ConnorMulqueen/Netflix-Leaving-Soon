{
    "manifest_version": 3,
    "name": "Netflix Leaving Soon",
    "description": "Adds a 'Leaving Soon' Titles button to Netflix",
    "version": "1.0",
    "author": "Connor Mulqueen",
    "action": {
      "default_popup": "main.html",
      "default_icon": "icon.png"
    },
    "content_scripts": [
        {
          "matches": ["https://*.netflix.com/*"],
          "js": ["content.js"]
        }
    ],
    "run_at": "document_end"
  }